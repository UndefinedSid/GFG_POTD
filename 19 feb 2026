Problem link -->>  

// C++ code

class Solution {
  public:
    vector<int> missinRange(vector<int>& arr, int low, int high) {
        // code here
        sort(arr.begin(),arr.end());
        vector<int> ans;
        unordered_set<int> st(arr.begin(),arr.end());
       
        for(int i=low;i<=high;i++){
            if(st.find(i)==st.end())
                ans.push_back(i);
        }
    
        return ans;
    }
};

// Python code

class Solution:
    def missinRange(self, arr, low, high):
        # Convert input array to a set for O(1) lookups
        present_elements = set(arr)
        
        # Create a range of all numbers we expect
        # and subtract the ones we have
        ans = [i for i in range(low, high + 1) if i not in present_elements]
        
        return ans
